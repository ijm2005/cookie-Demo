<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>CookieDemo</display-name>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file> 
  </welcome-file-list>
  
  <servlet>
<servlet-name>logical</servlet-name>
<servlet-class>com.nt.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>logical</servlet-name>
<url-pattern>/login</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>logical2</servlet-name>
<servlet-class>com.nt.InboxServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>logical2</servlet-name>
<url-pattern>/inbox</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>logical3</servlet-name>
<servlet-class>com.nt.LogoutServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>logical3</servlet-name>
<url-pattern>/logout</url-pattern>
</servlet-mapping>
  
  
</web-app>